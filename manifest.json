{
  "manifest_version": 2,
  "name": "DHL Autofiller",
  "description": "__MSG_extensionDescription__",
  "version": "1.4",
  "default_locale": "en",
  "permissions": [
        "tabs",
        "activeTab",
        "http://*/",
        "https://*/",
        "file:///"
    ],
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons" : {
    "16" : "img/icon-16.png",
    "48" : "img/icon-48.png",
    "128" : "img/icon-128.png"
  }
}